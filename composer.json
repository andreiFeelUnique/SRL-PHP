{
  "name": "simpleregex/srl-php",
  "description": "Simple Regex Language",
  "version": "0.1.0",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Karim Geiger",
      "email": "karim@tyvr.net"
    }
  ],
  "require": {
    "php": ">=5.0.0"
  },
  "autoload": {
    "psr-4": {
